wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.xpath(&apos;/html/body/app-root/div/div[1]/rz-search/rz-catalog/div/div[2]/section/rz-grid/ul/li[1]/app-goods-tile-default/div/div[2]/div[4]/div[2]/p/span[1]&apos;)));
var price1 = WDS.browser.findElement(org.openqa.selenium.By.xpath(&quot;/html/body/app-root/div/div[1]/rz-search/rz-catalog/div/div[2]/section/rz-grid/ul/li[1]/app-goods-tile-default/div/div[2]/div[4]/div[2]/p/span[1]&quot;)).getText();
WDS.browser.findElement(org.openqa.selenium.By.xpath(&quot;//a[2]/span&quot;)).click();
WDS.sampleResult.setResponseHeaders(&apos;isSuccess: false&apos;);
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.xpath(&quot;/html/body/app-root/div/div[1]/app-rz-product/div/product-tab-main/div[1]/div[1]/div[2]/product-main-info/div/div/div/p&quot;)));
var price2 = WDS.browser.findElement(org.openqa.selenium.By.xpath(&quot;/html/body/app-root/div/div[1]/app-rz-product/div/product-tab-main/div[1]/div[1]/div[2]/product-main-info/div/div/div/p&quot;)).getText();
price2 = price2.substring(0,price2.length()-1);
if(price1.equals(price2)){
	WDS.sampleResult.setResponseHeaders(&apos;isSuccess: true&apos;);
}
WDS.browser.navigate().back();
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.xpath(&apos;/html/body/app-root/div/div[1]/rz-search/rz-catalog/div/div[2]/section/rz-grid/ul/li[1]/app-goods-tile-default/div/div[2]/div[4]/div[2]/p/span[1]&apos;)));
